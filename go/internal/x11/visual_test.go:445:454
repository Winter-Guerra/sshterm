	// NOTE: This test is limited to a small subset of the logical operations
	// because the full test suite is too slow for the test environment.
	tests := []struct {
		name       string
		function   uint32
		wantPixels [3]uint8
	}{
		{"Copy", FunctionCopy, [3]uint8{255, 0, 0}},
		{"Xor", FunctionXor, [3]uint8{255, 0, 255}},
	}
